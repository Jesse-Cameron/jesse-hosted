<div class="index-mainbar">

    <div class="series-header">
        <h3><%= @album_map["title"] %></h3>
        <p><%= @album_map["subtitle"]%><p>
    </div>

    <%= for photo <- @album_map["images"] do %>
        <div class="photo-series">
            <%= img_tag(photo["url"],
                alt: photo["alt_text"],
                title: photo["alt_text"])
            %>
            <div class="subtitle"><%= photo["title"] %></div>
        </div>
    <% end %>
</div>